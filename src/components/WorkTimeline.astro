        .item-container:nth-of-type(odd) .item {
            flex-grow: 0; /* This item will only take up its content width */
            width: 50%; /* Set the width to 50% */
            margin-right: 52%; /* Push it to the right */
            margin-left: auto; /* Push it to the right */
        }
        .item.right,
