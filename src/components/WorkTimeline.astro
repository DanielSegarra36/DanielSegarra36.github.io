            justify-content: flex-end;
            align-items: center;
        }
        .item-container:nth-of-type(odd) .item + .image {
            /* right: 0; */
            left: 52%;
            justify-content: flex-start;
            align-items: center;
        }
        .image > * {
            height: 100%;
            border-radius: 20px;
            /* width: 100%; */
            object-fit: cover;
            max-width: 100%;
            max-height: 100%;
            /* object-fit: contain; */
            z-index: 1;
        }
    </style>
    <script
        is:inline
        src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.6.0/gsap.min.js"
    ></script>
    <script
        is:inline
        src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.6.0/ScrollTrigger.min.js"
    ></script>
    <script>
        gsap.registerPlugin(ScrollTrigger);

        // fade in line
        gsap.from("#line", {
            scrollTrigger: {
                trigger: "#line",
                start: "top 100%",
                end: "bottom bottom",
                toggleActions: "restart none none none",
            },
            opacity: 0,
            duration: 1,
        });

        // GSAP fade in all .right items from right
        gsap.utils
            .toArray(".item-container:nth-of-type(even) > *")
            .forEach((right) => {
                gsap.from(right, {
                    scrollTrigger: {
                        trigger: right,
                        start: "top 100%",
                        end: "bottom 200%",
                        scrub: 1,
                    },
                    x: 100,
                    opacity: 0,
                    duration: 1,
                    stagger: 3,
                });
                gsap.from(right.querySelectorAll(".image > *"), 30, {
                    scrollTrigger: {
                        trigger: right,
                        start: "top 80%",
                        end: "bottom 200%",
                        scrub: 1,
                    },
                    opacity: 0,
                    rotation: 90,
                    transformOrigin: "right bottom",
                    // if item's parent has id of metro-marine then stagger = 0 else stagger = 3
                    stagger: right.parentElement.id === "metro-marine" ? 0 : 3,
                    onComplete: function () {
                        if (right.parentElement.id === "metro-marine") {
                            right.classList.add("active");
                        }
                    },
                });
            });
        gsap.utils
            .toArray(".item-container:nth-of-type(odd) > *")
            .forEach((left) => {
                gsap.from(left, {
                    scrollTrigger: {
                        trigger: left,
                        start: "top 100%",
                        end: "bottom 200%",
                        scrub: 1,
                    },
                    x: -100,
                    opacity: 0,
                    duration: 1,
                    stagger: 3,
                });
                gsap.from(left.querySelectorAll(".image > *"), 30, {
                    scrollTrigger: {
                        trigger: left,
                        start: "top 80%",
                        end: "bottom 200%",
                        scrub: 1,
                    },
                    opacity: 0,
                    rotation: -90,
                    transformOrigin: "left bottom",
                    stagger: 3,
                });
            });
    </script>
</div>
